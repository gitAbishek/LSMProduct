  "scripts": {
    "start": "webpack-dev-server --mode development --hot --config ./webpack/webpack.config.dev.js",
    "build": "webpack --mode production --config ./webpack/webpack.config.prod.js && gulp build",
    "build:analyze": "webpack --mode production --env.addons=bundleanalyzer --config ./webpack/webpack.config.prod.js",
    "dev:blocks": "webpack --watch --mode development --config ./webpack/webpack.blocks.dev.js",
    "build:blocks": "webpack --mode production --config ./webpack/webpack.blocks.prod.js && gulp build",
    "dev": "gulp dev",
    "release": "yarn build && yarn build:blocks && gulp release",
    "lint": "eslint ./assets/js --ext ts,tsx",
    "prettier": "prettier --check ./assets/js/**/*.tsx ./assets/js/**/*.ts",
    "prepare": "husky install",
    "reactivate": "wp plugin deactivate learning-management-system && wp plugin activate learning-management-system ",
    "switchDev": "replace-in-file \"define( 'MASTERIYO_DEVELOPMENT', false);\" \"define( 'MASTERIYO_DEVELOPMENT', true);\" ../../../wp-config.php",
    "switchProd": "replace-in-file \"define( 'MASTERIYO_DEVELOPMENT', true);\" \"define( 'MASTERIYO_DEVELOPMENT', false);\" ../../../wp-config.php",
    "makepot": "composer run makepot",
    "test:php:unit": "composer run test"
  }
